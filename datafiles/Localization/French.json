{
    "Player Characters": {
        "Kirby": {
            "Title": "Kirby",
            "Description": "Arrivant avec la brise de printemps, c'est Kirby ! Avalez des ennemis et copiez leurs pouvoirs. Une galaxie s'étend au-delà de vos plus grands rêves, il est temps d'y aller !"
        },
        "Gamble": {
            "Title": "Gamble",
            "Description": "Ce nouvel ami d'un espace lointain est venu sur la planète Pop demander à Kirby un coup de main pour vaincre le Fluxbender cosmique ! Gamble utilise des dés, cartes, roulettes, et de l'énergie magique contre ses ennemis. Pliez-les en deux !"
        },
        "Meta Knight": {
            "Title": "Meta Knight",
            "Description": "Le chevalier masqué mystérieux de Kirby's Adventure est là ! Il est toujours respectueux et combat toutes les sources du mal. L'entièreté de Dream Land redoute son épée !"
        },
        "Helper": {
            "Title": "Acolyte",
            "Description": "Des amis de toute part se sont rassemblés pour venir en aide à Kirby dans son aventure ! Jouez avec une multitude de créatures provenant de la galaxie Gamble, de petits Waddle Dee aux Minérissons invincibles !"
        },
        "Gooey": {
            "Title": "Gooey",
            "Description": "Outre son origine étrange, Gooey possède un cœur d'or, étant toujours présent pour ses amis. À l'aide de sa langue beaucoup trop longue, il est heureux d'aider Kirby dans son aventure !"
        },
        "Magolor": {
            "Title": "Magolor",
            "Description": "Du flux ? Je vais vous montrer de la vraie puissance !' Ce magicien sournois arrive de Kirby's Adventure Wii. Il était autrefois un menteur sans scrupule, mais il a changé... pas vrai ? Sa magie extraordinaire va faire succomber les ennemis !"
        },
        "Keeby": {
            "Title": "Keeby",
            "Description": "Bien qu'il ne soit pas aussi aventureux que son ami rival, Keeby semblait vouloir l'accompagner dans sa quête ! Peut-il prendre une pause dans sa carrière de golf ?"
        }
    },
    "Player Abilities": {
        "Cutter": {
            "Title": "Trancheur",
            "Description": "Utilise un boomerang aiguisé pour trancher et foncer à traversles ennemis !"
        },
        "Beam": {
            "Title": "Rayon",
            "Description": "Projette des rayons d’énergie aux alentours, ou lance uneattaque chargée à distance !"
        },
        "Mystic Beam": {
            "Title": "Rayon Mystique",
            "Description": "Le pouvoir du flux a transformé le Rayon en Rayon Mystique ! Le bouclier peut être utilisé comme attaque !"
        },
        "Stone": {
            "Title": "Rocher",
            "Description": "Transforme-toi en pierre et deviens impénétrable ! Mais quelles sont ces étranges statues ?"
        },
        "Ufo": {
            "Title": "OVNI",
            "Description": "Transforme-toi en vaisseau alien et détruis tes ennemis avec des lasers, Piou Piou !"
        },
        "Mirror": {
            "Title": "Miroir",
            "Description": "Utilise le pouvoir des reflets et renvoie leurs attaques aux ennemis !"
        },
        "Ninja": {
            "Title": "Ninja",
            "Description": "Deviens un assassin des ombres et frappe en toute discrétion !"
        },
        "Bomb": {
            "Title": "Bombe",
            "Description": "Prends une bombe et lance-la, mais ne la tiens pas trop longtemps !"
        },
        "Fire": {
            "Title": "Feu",
            "Description": "Brûle la compétition avec de puissantes flammes, outch !"
        },
        "Mystic Fire": {
            "Title": "Feu Mystique",
            "Description": "Le pouvoir du flux a rendu ton Feu plus puissant ! Plus besoin de viser, réduis tes ennemis en cendres !"
        },
        "Ice": {
            "Title": "Glace",
            "Description": "Patine et gèle tes ennemis avec ton souffle glacé !"
        },
        "Spark": {
            "Title": "Étincelle",
            "Description": "Appuies sur différentes directions pour charger de l’électricité et foudroie les ennemis."
        },
        "Yoyo": {
            "Title": "Yoyo",
            "Description": "Glisse dans l’action et lance un puissant yoyo avec style !"
        },
        "Wheel": {
            "Title": "Roue",
            "Description": "Fonce à travers tes ennemis en étant presque invincible, vroum-vroum !"
        },
        "Artist": {
            "Title": "Artiste",
            "Description": "Frappe tes ennemis avec ton pinceau ou peins une armée de dessins qui combattra à tes côtés !"
        },
        "Fighter": {
            "Title": "Combat",
            "Description": "Deviens un maître des arts martiaux et mets tes adversaires à terre !"
        },
        "Suplex": {
            "Title": "Lutteur",
            "Description": "Manifeste l’esprit du lutteur en saisissant et lançant les ennemis !"
        },
        "Wing": {
            "Title": "Aile",
            "Description": "Vole dans les airs avec grâce. Fonce sur les ennemis ou lance une volée de plumes vers eux !"
        },
        "Jet": {
            "Title": "Jet",
            "Description": "Charge le moteur pour t’envoler en avant à toute vitesse !"
        },
        "Sword": {
            "Title": "Épée",
            "Description": "Brandis une lame tranchante pour couper et percer toutes sortes de choses !"
        },
        "Parasol": {
            "Title": "Parasol",
            "Description": "Protège-toi des objets tombants, claque les ennemis, et ralentis les chutes !"
        },
        "Hammer": {
            "Title": "Marteau",
            "Description": "Fracasse les ennemis en secouant violemment un marteau, aïe !"
        },
        "Bell": {
            "Title": "Clochette",
            "Description": "Ding-dong ! Manie ces deux bruyants instruments pour composer une chanson destructrice !"
        },
        "Water": {
            "Title": "Eau",
            "Description": "Contrôle l’eau à ta guise pour emporter les ennemis. Surfez !"
        },
        "Sleep": {
            "Title": "Sommeil",
            "Description": "Bâille... Encore 5 minutes... zzzzzzzzzzz..."
        },
        "Scan": {
            "Title": "Copieur",
            "Description": "Utilise un casque high-tech pour copier les capacités des ennemis !"
        },
        "Crash": {
            "Title": "Foudre",
            "Description": "Balaye tous les ennemis à l’écran avec une puissante explosion !"
        },
        "Mic": {
