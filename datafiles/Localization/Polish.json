{
    "Player Characters":
    {
        "Kirby":
        {
            "Title": "Kirby",
            
            "Description": "A niech mnie bryza rozpręży, to Kirby! Wchłaniaj przeciwników oraz kopiuj ich moce. Galaktyka rozwija się ponad twoimi szalonymi marzeniami, więc czas się ruszyć!"
        },
        
        "Gamble":
        {
            "Title": "Gamble",
            
            "Description": "Ten nowy koleżka z odległej galaktyki zjawił się na Popstar żeby poprosić Kirby'ego o pomocną dłoń w pokonaniu Fluxbendera! Gamble trzyma ze sobą kości, karty, ruletki oraz magiczną energie żęby pokonać wrogów. Złożyć ich jak papier!"
        },
        
        "Meta Knight":
        {
            "Title": "Rycerz Meta",
            
            "Description": "Ten zamaskowany rycerz z Przygody Kirby'ego tu jest! Jest on rycerski oraz zawsze gotowy do walki z jakimkolwiek złem. Cała Kraina Marzeń boi się jego ostrza!"
        },
        
        "Helper":
        {
            "Title": "Pomocnicy",
            
            "Description": "Znajomi z każdego miejsca przyszli żeby razem pomóc Kirby'emu w jego przygodzie! Graj bogactwem stworzeń z całej Galaktyki Gamble, od przysadzistych Waddle Dee do niepokonanych Jeżowców!"
        },
        
        "Gooey":
        {
            "Title": "Gooey",
            
            "Description": "Nawet jeśli ma dziwaczną przeszłość, Gooey ma serce ze złota, zawsze pomocny jego znajomym. Z jego komicznie długim językiem, jest zawsze chętny do udziału w przygodzie z Kirby'm i jego przyjacielom!"
        },
        
        "Magolor":
        {
            "Title": "Magolor",
            
            "Description": "Flux? Ja ci pokaże prawdziwą moc! Ten podstępny czarodziej pochodzi z 'Kirby's Return To Dream Land'. Kiedyś był kłamiącym oszustem, ale teraz się zreformował.... racja? Jego ponadgalaktyczna moc sprawi że z przeciwników wyjdzie popiół!"
        },
        
        "Keeby":
        {
            "Title": "Keeby",
            
            "Description": "Chociaż że nie jest aż tak śmiały jak jego przyjazny rywal, Keeby wydaje się chętny dołączenia do jego wyprawy! Czy on sobie zrobi przerwe z jego kariery golfowej?"
        }
    },
	
	"Player Abilities":
    {
        "Cutter":
        {
            "Title": "Krajacz",
            
            "Description": "Władaj ostrym jak żyletka bumerangiem żeby tnąć i przecinać się przez przeszkody!"
        },
		
		"Beam":
        {
            "Title": "Promień",
            
            "Description": "Wyczaruj elektryczne promienie żeby trzepać nimi dookoła. załaduj atak żeby wystrzelić z daleka!"
        },
		
		"Mystic Beam":
        {
            "Title": "Mityczny Promień",
            
            "Description": "Poprzez moce Flux'a zmieniło nasz promień w Promień Mityczny! Tarcza z Promienia podwaja się jako atak!"
        },
		
		"Stone":
        {
            "Title": "Głaz",
            
            "Description": "Zmień się w głaz i stań się praktycznie nietykalny! Czym są te dziwaczne posągi?"
        },
		
		"Ufo":
        {
            "Title": "UFO",
            
            "Description": "Zamień się w statek kosmiczny i rozstrzelaj złe istoty laserami, piu, piu!"
        },
		
		"Mirror":
        {
            "Title": "Lustro",
            
            "Description": "Użyj mocy odzwierciedleń żeby oddać pociski przeciwników z powrotem do nadawcy!"
        },
		
		"Ninja":
        {
            "Title": "Ninja",
            
            "Description": "Stań się zabójcą cieni i uderzaj bez zauważenia!"
        },
		
		"Bomb":
        {
            "Title": "Bomba",
            
            "Description": "Złap bombę w ręce i wyrzuć nią ale uważaj, nie trzymaj ją za długo! "
        },
		
		"Fire":
        {
            "Title": "Ogień",
            
            "Description": "Spal swoją konkurencje płomieniami gorącymi jak słońce, parzy!"
        },
		
		"Mystic Fire":
        {
            "Title": "Mityczny Ogień",
            
            "Description": "Moce Flux'a sprawiły że twój dobrze znany ogień jest teraz znacznie silniejszy! Nie trzeba teraz celować, więc wypal swoich przeciwników dopóki będzie tylko proch na ziemi!"
        },
		
		"Ice":
        {
            "Title": "Lód",
            
            "Description": "Ślizgaj się i zamarzaj twoje przeszkody swoim lodowatym oddechem i nie tylko!"
        },
		
		"Spark":
        {
            "Title": "Iskra",
            
            "Description": "Roztrzep się na lewo i prawo żeby się naelektryzować i zastrzelić twoich przeciwników czystą energią "
        },
		
		"Yoyo":
        {
            "Title": "Jojo",
            
            "Description": "Wsuń się w akcje i rozmachuj swoim jojo z modną mocą! "
        },
		
		"Wheel":
        {
            "Title": "Koło",
            
            "Description": "Przejeżdżaj tłumy Przeciwników podczas gdy jesteś prawie nietykalny, brum brum!"
        },
		
		"Artist":
        {
            "Title": "Malarz",
            
            "Description": "Uderzaj swoich przeciwników twoim magicznym pędzlem lub namaluj armię rysunków żeby walczyli po "
        },
		
		"Fighter":
        {
            "Title": "Wojownik",
            
            "Description": "Zmień się w mistrza sztuk walki i zadaj K.O twoim przeciwnikom, a-masz!"
        },
		
		"Suplex":
        {
            "Title": "Supleks",
            
            "Description": "Wciel w sobie Duszę prawdziwego zapaśnika poprzez łapanie przeciwników i przerzucanie nimi na różne strony! "
        },
		
		"Wing":
        {
            "Title": "Skrzydłą",
            
            "Description": "Wznieś się swobodnie ku powietrzu i przebijaj się przez przeciwników lub wyrzucaj salwy piór ostrych jak żyletka prosto na żywe przeszkody!"
        },
		
		"Jet":
        {
            "Title": "Odrzutowiec",
            
            "Description": "Załaduj silnik i przeleć w te i nazad w szybkości dźwięku!"
        },
		
		"Sword":
        {
            "Title": "Miecz",
            
            "Description": "Władaj ostrzem i rozcinaj przeciwników jak nóż przez masło! "
        },
		
		"Parasol":
        {
            "Title": "Parasol",
            
            "Description": "Broń się przed spadającymi pociskami, uderzaj przeciwników oraz spadaj powoli, jak piórko!"
        },
		
		"Hammer":
        {
            "Title": "Młot",
            
            "Description": "Zgnieć przeciwników z ekstremalnie silnymi trzepnięciami młota, auć! "
        },
		
		"Bell":
        {
            "Title": "Dzwon",
            
            "Description": "Dzyń dzyń! Władaj dwuręcznie głośnymi dzwonkami żeby utworzyć twoją własną ode do zniszczenia!"
        },
		
		"Water":
        {
            "Title": "Woda",
            
            "Description": "Ukształtuj wode poprzez twoją wolną wole i zrób ostateczną kąpiel twoim wrogom. Uwaga, będzie "
